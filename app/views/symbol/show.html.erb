<div style="font:12px tahoma,arial,sans-serif;">
  <span id="symbol_name"><%= @symbol %></span>
  -
  <span id="symbol_comment"><%=
      @comment.gsub(/#\$[\w|-]+/) {|c| "<a href=\"\">#{c[2..-1]}</a>"} 
  %></span>

  <div>
    Generalize - 
    <% for genls in @all_genls %>
        <a href="#"><%= genls %></a>&nbsp;
    <% end %>

  </div>
</div>
